import java.util.*;
public class question1{

	public static void main(String[] args) {
		int[] arr= {6,3,8,4,2,9,1,6};
		int l=0;
		int r=arr.length-1;
		int sum=0;
		for(int i=0;i<arr.length;i++) {
			while(l<r){
			if(arr[l]!=6) {
				sum+=arr[i];
				l++;
			}
			if(arr[r]!=7) {
				sum+=arr[i];
				r--;
			}
			if(arr[l]==6 &&arr[r]==7) {
				break;
			}
		}
		
	}
		System.out.println("Sum: "+sum);
}
}
