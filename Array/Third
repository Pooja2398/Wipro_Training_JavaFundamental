import java.util.*;
import java.lang.Math;   

public class question1{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr= {6,7,4,3,9,1,5,9};
		double n;
		n= Math.random();
		int index = -1;
		for(int i=0;i<arr.length;i++) {
			if(arr[i]==n) {
				index=i+1;
			    break;
			}
		}
		System.out.println("Index: "+index);
	}

}
