import java.util.*;
public class question1{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr= {6,7,4,3,9,1,5,9};
		Arrays.sort(arr);
		int min=arr[0];
		int max=arr[arr.length-1];
		System.out.println("Minimum:"+min);
		System.out.println("Maximum:"+max);
	}

}
